{
    "name": "draco.DracoPipeline",
    "primitive": "draco.DracoPipeline",
    "fit": {
        "method": "fit",
        "args": "get_fit_args"
    },
    "produce": {
        "method": "predict",
        "args": "get_predict_args",
        "output": "get_output_args"
    },
    "hyperparameters": {
        "fixed": {
            "templates": {
                "type": "list",
                "default": []
            },
            "metric": {
                "type": "str",
                "default": "accuracy"
            },
            "cost": {
                "type": "bool",
                "default": false
            },
            "init_params": {
                "type": "dict",
                "default": {}
            },
            "stratify": {
                "type": "bool",
                "default": true
            },
            "cv_splits": {
                "type": "int",
                "default": 5
            },
            "shuffle": {
                "type": "bool",
                "default": true
            },
            "random_state": {
                "type": "int",
                "default": 0
            },
            "preprocessing": {
                "type": "int",
                "default": 0
            },
            "cache_path": {
                "type": "str",
                "default": null
            },
            "threshold": {
                "type": "float",
                "default": null
            }
        }
    }
}